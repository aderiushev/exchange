{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "credentialsSource": "remote"
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "developmentClient": false,
      "ios": { "resourceClass": "m-medium", "simulator": false },
      "android": { "buildType": "app-bundle" },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
